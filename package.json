{
  "name": "unacl",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@7.5.1",
  "description": "Minimal, type-safe and reactive ACL implementation",
  "author": {
    "name": "Johann Schopplich",
    "email": "pkg@johannschopplich.com",
    "url": "https://johannschopplich.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/unacl#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/unacl.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/unacl/issues"
  },
  "keywords": [
    "access-control",
    "acl",
    "roles",
    "permissions",
    "reactive"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp --commit --push --tag",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@vue/reactivity": "^3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "bumpp": "^8.2.1",
    "eslint": "^8.19.0",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.4",
    "vitest": "^0.18.0"
  }
}
