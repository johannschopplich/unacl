{
  "name": "unacl",
  "type": "module",
  "version": "0.2.0",
  "packageManager": "pnpm@7.12.2",
  "description": "Minimal, type-safe and reactive ACL implementation",
  "author": {
    "name": "Johann Schopplich",
    "email": "pkg@johannschopplich.com",
    "url": "https://johannschopplich.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/unacl#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/unacl.git"
  },
  "bugs": {
    "url": "https://github.com/johannschopplich/unacl/issues"
  },
  "keywords": [
    "access-control",
    "acl",
    "roles",
    "permissions",
    "reactive"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp --commit --push --tag",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@vue/reactivity": "^3.2.39"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.23.1",
    "typescript": "^4.8.3",
    "unbuild": "^0.8.11",
    "vitest": "^0.23.4"
  }
}
